{
    "name": "man-page-preview",
    "displayName": "Unix man page preview",
    "description": "Displays a live preview of Unix `man` pages.",
    "version": "0.0.1",
    "publisher": "-",
    "license": "GPL-3.0+",
    "engines": {
        "vscode": "^1.20.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showManPagePreview"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showManPagePreview",
                "title": "Show Unix man page Preview"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^1.1.6"
    }
}
